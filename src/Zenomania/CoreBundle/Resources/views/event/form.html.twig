{% extends 'base.html.twig' %}
{% form_theme form _self %}

{% block javascripts %}
    {% javascripts
    'bundles/zenomaniacore/system/moment/min/moment-with-locales.min.js'
    'bundles/zenomaniacore/system/plugins/datetimepicker/js/bootstrap-datetimepicker.min.js'
    'bundles/zenomaniacore/plugins/datepicker/settings.js'
    'bundles/zenomaniacore/system/plugins/select2/select2.js'
    'bundles/zenomaniacore/system/plugins/select2/i18n/ru.js'
    'bundles/tetranzselect2entity/js/select2entity.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block stylesheets %}
    {% stylesheets
    'bundles/zenomaniacore/system/plugins/datetimepicker/css/bootstrap-datetimepicker.css'
    'bundles/zenomaniacore/plugins/datepicker/style.css'
    'bundles/zenomaniacore/system/plugins/select2/select2.css'
    'bundles/zenomaniacore/plugins/select2/style.css'
    filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
{% endblock %}